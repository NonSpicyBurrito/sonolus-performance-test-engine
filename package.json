{
  "name": "sonolus-performance-test-engine",
  "version": "1.0.0",
  "description": "An engine for testing performance of Sonolus",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:play-parallel": "sonolus-cli --dev ./play/sonolus-cli.config.parallel.js",
    "dev:play-sequential": "sonolus-cli --dev ./play/sonolus-cli.config.sequential.js",
    "prebuild": "tsc -p ./shared --noEmit && tsc -p ./play --noEmit && eslint --ext .ts ./shared && eslint --ext .ts ./play",
    "build": "sonolus-cli --build ./play/sonolus-cli.config.parallel.js && sonolus-cli --build ./play/sonolus-cli.config.sequential.js"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.2",
    "prettier-plugin-organize-imports": "^3.2.3",
    "sonolus-core": "~7.2.0",
    "sonolus.js": "~9.1.0",
    "typescript": "~5.1.6"
  }
}
