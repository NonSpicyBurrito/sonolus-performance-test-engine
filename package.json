{
  "name": "sonolus-performance-test-engine",
  "version": "1.1.0",
  "description": "An engine for testing performance of Sonolus",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:play-parallel": "sonolus-cli --dev ./play/sonolus-cli.config.parallel.js",
    "dev:play-sequential": "sonolus-cli --dev ./play/sonolus-cli.config.sequential.js",
    "dev:tutorial": "sonolus-cli --dev ./tutorial",
    "prebuild": "tsc -p ./shared --noEmit && tsc -p ./play --noEmit && tsc -p ./tutorial --noEmit && eslint . && prettier . --check",
    "build": "sonolus-cli --build ./play/sonolus-cli.config.parallel.js && sonolus-cli --build ./play/sonolus-cli.config.sequential.js && sonolus-cli --build ./tutorial"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "sonolus-core": "~7.3.0",
    "sonolus.js": "~9.2.0",
    "typescript": "~5.2.2"
  }
}
