{
  "name": "sonolus-performance-test-engine",
  "version": "1.0.0",
  "description": "An engine for testing performance of Sonolus",
  "private": true,
  "type": "module",
  "scripts": {
    "dev-parallel": "sonolus-cli --dev --parallel",
    "dev-sequential": "sonolus-cli --dev --sequential",
    "prebuild": "tsc -p . --noEmit && eslint --ext .ts ./src",
    "build": "sonolus-cli --build --parallel && sonolus-cli --build --sequential"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "sonolus-core": "~7.0.0",
    "sonolus.js": "~9.0.0",
    "typescript": "~5.1.3"
  }
}
